<?xml version="1.0"?>
 <!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" > 

    <hibernate-mapping>
    
    <class name="com.Fertilizer.hibernate.GrDetailsBean" 	
    table="gr_details" schema="process_soft"
    >
    
    	<id name="pk_GRDetails_id" type="java.lang.Long" column="pk_gr_id">
			<generator class="increment" />
		</id>


		<property name="idno" type="java.lang.String" column="idno" />
		<property name="suppliername" type="java.lang.String" column="suppliername" />
		<property name="date" type="java.util.Date" column="date" />
		<property name="contactno" type="java.lang.Long" column="contactno" />
		<property name="city" type="java.lang.String" column="city" />
		<property name="gstno" type="java.lang.String" column="gstno" />
		<property name="address" type="java.lang.String" column="address" />
		<property name="productname" type="java.lang.String" column="productname" />
		<property name="operation" type="java.lang.String" column="operation" />
		<property name="quantity" type="java.lang.Double" column="quantity" />
		
		<property name="GridTotal" type="java.lang.Double" column="GridTotal" />
		<property name="rate" type="java.lang.Double" column="rate" />
		<property name="discount" type="java.lang.Double" column="discount" />
		<property name="gst" type="java.lang.Double" column="gst" />
		<property name="igst" type="java.lang.Double" column="igst" /> 
		<property name="TaxAmount" type="java.lang.String" column="TaxAmount" /> 
		<property name="unit" type="java.lang.String" column="unit" />
		<property name="total" type="java.lang.Double" column="total" />
		<property name="grosstotal" type="java.lang.Double" column="grosstotal" />
		<property name="ShopName" type="java.lang.String" column="Shop" />
		<property name="billno" type="java.lang.Double" column="billno" />
		
		<property name="userId" type="java.lang.Long" column="user_id"></property>
		<property name="username" type="java.lang.String" column="username"></property>
		<property name="shopId" type="java.lang.Long" column="shop_id"></property>
		
		</class>
    </hibernate-mapping>